{
  "name": "myvault",
  "version": "1.0.0",
  "description": "A full-stack file and notes management application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["vault", "files", "notes", "react", "nodejs", "mongodb"],
  "author": "SathwikUK",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 